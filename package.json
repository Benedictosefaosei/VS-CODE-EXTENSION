{
  "name": "benedict",
  "displayName": "Benedict",
  "publisher": "benedictoseisefa",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.addQuestion",
    "onCommand:extension.editQuestion",
    "onCommand:extension.answerQuestion",
    "onCommand:extension.editAnswer",
    "onCommand:extension.viewQA",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.addQuestion",
        "title": "Quiz: Add Question on Selection"
      },
      {
        "command": "extension.editQuestion",
        "title": "Quiz: Edit Question"
      },
      {
        "command": "extension.answerQuestion",
        "title": "Quiz: Answer Question"
      },
      {
        "command": "extension.editAnswer",
        "title": "Quiz: Edit Answer"
      },
      {
        "command": "extension.viewQA",
        "title": "Quiz: View Questions & Answers"
      },
      {
        "command": "quiz.createConfigFile",
        "title": "Quiz: Create Config File"
      },
      {
        "command": "extension.generateQA",
        "title": "Quiz: Generate QA"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./",
    "test": "npm run compile && node ./out/test/runTest.js",
    "test-ui": "npm run compile && vscode-test"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "^18.x",
    "@types/uuid": "^10.0.0",
    "@types/vscode": "^1.85.0",
    "@vscode/test-electron": "^2.5.2",
    "glob": "^11.0.3",
    "mocha": "^10.8.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  }
}
